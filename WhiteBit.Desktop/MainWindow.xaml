<Window x:Class="WhiteBit.Desktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
      <StackPanel>
          <StackPanel Orientation="Horizontal">
              <Label>SOL/USDT</Label>
              <Label Width="100" Content="{Binding SolUsdt}" MouseLeftButtonUp="LabelSolClicked"></Label>
          </StackPanel>
          <StackPanel Orientation="Horizontal">
              <Label>BTC/USDT</Label>
              <Label Width="100" Content="{Binding BtcUsdt}" MouseLeftButtonUp="LabelBtcClicked"></Label>
          </StackPanel>
      </StackPanel>
    </Grid>
</Window>
